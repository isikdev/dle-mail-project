t d LEFT JOIN dle_post_extras e ON (d.id=e.news_id)  INNER JOIN (SELECT dle_post_extras_cats.news_id, GROUP_CONCAT(dle_post_extras_cats.cat_id) as ci FROM dle_post_extras_cats GROUP BY news_id HAVING (FIND_IN_SET('6', ci) > 0) AND (FIND_IN_SET('93', ci) > 0 OR FIND_IN_SET('19', ci) > 0) AND (NOT FIND_IN_SET('8', ci) > 0)) z ON (d.id=z.news_id) WHERE approve=1  ORDER BY date desc LIMIT 0,48) as l ON p.id=l.id ORDER BY date desc","foundNews":1,"countNews":"12","haveRobots":false}