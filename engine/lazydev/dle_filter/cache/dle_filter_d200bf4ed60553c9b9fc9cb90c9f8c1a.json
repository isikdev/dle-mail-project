utor, p.date, p.short_story, CHAR_LENGTH(p.full_story) as full_story, p.xfields, p.title, p.category, p.alt_name, p.comm_num, p.allow_comm, p.fixed, p.tags, e.news_read, e.allow_rate, e.rating, e.vote_num, e.votes, e.view_edit, e.editdate, e.editor, e.reason FROM dle_post p LEFT JOIN dle_post_extras e ON (p.id=e.news_id)  JOIN (SELECT d.id FROM dle_post d LEFT JOIN dle_post_extras e ON (d.id=e.news_id)  INNER JOIN (SELECT dle_post_extras_cats.news_id, GROUP_CONCAT(dle_post_extras_cats.cat_id) as ci FROM dle_post_extras_cats GROUP BY news_id HAVING (FIND_IN_SET('74', ci) > 0) AND (FIND_IN_SET('6', ci) > 0) AND (NOT FIND_IN_SET('8', ci) > 0)) z ON (d.id=z.news_id) WHERE approve=1  ORDER BY e.editdate desc LIMIT 0,48) as l ON p.id=l.id ORDER BY e.editdate desc","foundNews":1,"countNews":"42","haveRobots":false}